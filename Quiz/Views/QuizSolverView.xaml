<Window x:Class="Quiz.Views.QuizSolverView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Quiz.Views"
        mc:Ignorable="d"
        Title="Rozwiązywanie Quizu" Height="400" Width="600">
    <Grid>
        <StackPanel>
            <TextBlock Text="Widok Rozwiązywania Quizu" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="24" />
            <ItemsControl ItemsSource="{Binding Questions}" />
            <Button Content="Menu" Command="{Binding NavigateBackCommand}" HorizontalAlignment="Center" Margin="10" />
        </StackPanel>
    </Grid>
</Window>
